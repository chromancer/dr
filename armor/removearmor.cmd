removearmor:

	echo * * * Removing armor.
	put remove netting
	put stow netting
	pause 1
	put remove gloves
	put stow gloves
	pause 1
	put remove cowl
	put stow cowl
	pause 1
	put remove shield
	put stow shield
	pause 1
	put remove my stick
	put put my stick in my bag
	pause 1
	put remove knuckle
	put stow knuckle
	pause 1
	echo * * * Done.
	exit
